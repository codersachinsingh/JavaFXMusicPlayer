<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="70.0" prefWidth="800.0" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="musicplayer.ui.fxmls.AlbumsCellController">
   <padding>
      <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
   </padding>
   <children>
      <ImageView fx:id="albumart" fitHeight="55.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true" />
      <VBox prefHeight="60.0" prefWidth="981.0">
         <children>
            <Label fx:id="albumtitle" prefHeight="28.0" prefWidth="710.0" text="Ajab Prem Ki Ghazab Kahani">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="artist" prefHeight="28.0" prefWidth="254.0" text="Atif Aslam">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="totalSongs" prefHeight="28.0" prefWidth="214.0" text="7 Songs" />
                  <Label fx:id="year" prefHeight="28.0" prefWidth="173.0" text="2009" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
